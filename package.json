{
  "name": "@material-appkit/quickstart-gatsby",
  "description": "Gatsby Quickstart",
  "version": "1.0.0",
  "author": "Allan Hart",
  "dependencies": {
    "@fontsource/open-sans": "^4.1.0",
    "@material-appkit/core": "^2.9.92",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "gatsby": "^2.29.3",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-material-ui": "^2.1.10",
    "gatsby-plugin-react-helmet-async": "^1.1.0",
    "gatsby-source-filesystem": "^2.8.1",
    "notistack": "^1.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7"
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_APP_VERSION=$npm_package_version gatsby build",
    "develop": "gatsby develop -H 0.0.0.0 -p 8001",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "GATSBY_APP_VERSION=$npm_package_version npm run develop",
    "serve": "gatsby serve",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://#"
  },
  "bugs": {
    "url": "https://#"
  }
}
